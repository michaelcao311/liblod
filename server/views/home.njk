<!DOCTYPE html>
<head>
  <title>blod</title>
  <link href="https://fonts.googleapis.com/css?family=Bungee+Shade|Source+Sans+Pro:300" rel="stylesheet">
  <link href="/css/home.css" rel="stylesheet">
  <script src="http://code.jquery.com/jquery-1.11.1.js"></script>
  <script src="nunjucks.js"></script>
  <script src="/home.js"></script>
<head>
<body>
    <h1 id="header"> Welcome to Liblod! </h1>
  <br>
  <form action="/reg" method="get">
    <h3> <strong>Name: </strong> </h3>
    <br>
    <input id="name" type="text" name="name">
    <br>
    <h3> <strong>Room: </strong> </h3>
    <br>
    <input id="roomtyper" type="text" name="room">
    <br>
    <select id="roomselector" name="room">
      <option> -- </option>
      {% for room in renderedRooms %}
      <option class="roomoption" value="{{ room }}">
        {{ room }}
      </option>
      {% endfor %}
    </select> 
    <input id="submitter" type="submit" value="Submit!"> 


  </form>

<!--     <script src="/socket.io/socket.io.js"></script>
  <script src="client.js"></script>  -->

</body>
